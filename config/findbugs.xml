<FindBugsFilter>

    <Match>
        <Class name="~.*\.R\$.*" />
    </Match>
    <Match>
        <Class name="~.*\.Manifest\$.*" />
    </Match>
    <Match>
        <Class name="~.*\.*Test" />
        <Not>
            <Bug code="IJU" />
        </Not>
    </Match>
    <Match>
        <Bug pattern="DM_DEFAULT_ENCODING" />
    </Match>

</FindBugsFilter>